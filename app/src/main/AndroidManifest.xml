<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="gt.maggarzona.imagensoyfri">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/mgr"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mgr"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImagenSoyFRI"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".InformacionUsuarioOL"/>
        <activity android:name=".InformacionUsuario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>